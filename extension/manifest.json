{
  "manifest_version": 3,
  "name": "FastCache",
  "version": "0.5.0",
  "author": "Stephen Lee",
  "description": "Cache-like Bookmark Google Chrome Extension ",
  "homepage_url": "",
  "host_permissions": [ "<all_urls>" ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "./content.js" ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "View, visit, and delete your links quickly"
  }
}
