{
  "manifest_version": 3,
  "name": "FastCache",
  "version": "1.0.0",
  "author": "Stephen Lee",
  "description": "Cache-like Bookmark Google Chrome Extension ",
  "permissions": ["tabs", "storage"],
  "host_permissions": [ "<all_urls>" ],
  "icons": {
    "16": "./icons/FastCache.png",
    "48": "./icons/FastCache.png",
    "128": "./icons/FastCache.png"
  },
  "action": {
    "default_popup": "./build/index.html",
    "default_title": "FastCache"
  }
}
